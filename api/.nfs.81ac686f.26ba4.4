

<VirtualHost *:443>
  DocumentRoot "www/Code/ocr/pdf-to-ocr/"
  ServerName "ocr.enter.lan"
  SSLCertificateFile "etc/apache24/ssl/acc.crt"
  SSLCertificateKeyFile "etc/apache24/ssl/acc.key"
  <Directory "/usr/local/www/Code/ocr/pdf-to-ocr">
    AllowOverride FileInfo
    Require all granted
    Options FollowSymLinks
  </Directory>
</VirtualHost>


